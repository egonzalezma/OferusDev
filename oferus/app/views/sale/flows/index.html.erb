<p id="notice"><%= notice %></p>

<h1>Listing Sale Flows</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sale_flows.each do |sale_flow| %>
      <tr>
        <td><%= link_to 'Show', sale_flow %></td>
        <td><%= link_to 'Edit', edit_sale_flow_path(sale_flow) %></td>
        <td><%= link_to 'Destroy', sale_flow, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Flow', new_sale_flow_path %>

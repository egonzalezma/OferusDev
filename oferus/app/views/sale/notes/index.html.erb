<p id="notice"><%= notice %></p>

<h1>Listing Sale Notes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sale_notes.each do |sale_note| %>
      <tr>
        <td><%= link_to 'Show', sale_note %></td>
        <td><%= link_to 'Edit', edit_sale_note_path(sale_note) %></td>
        <td><%= link_to 'Destroy', sale_note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Note', new_sale_note_path %>

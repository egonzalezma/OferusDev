<p id="notice"><%= notice %></p>

<h1>Listing Sale Note Items</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sale_note_items.each do |sale_note_item| %>
      <tr>
        <td><%= link_to 'Show', sale_note_item %></td>
        <td><%= link_to 'Edit', edit_sale_note_item_path(sale_note_item) %></td>
        <td><%= link_to 'Destroy', sale_note_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Item', new_sale_note_item_path %>

<p id="notice"><%= notice %></p>

<h1>Listing Team Reports</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @team_reports.each do |team_report| %>
      <tr>
        <td><%= link_to 'Show', team_report %></td>
        <td><%= link_to 'Edit', edit_team_report_path(team_report) %></td>
        <td><%= link_to 'Destroy', team_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Report', new_team_report_path %>
